<?xml version="1.0" encoding="UTF-8"?>
<jboss-web>
    <!-- A reference to an EJB in the same server with a custom JNDI binding -->
   <ejb-ref>
        <ejb-ref-name>RSASignSession</ejb-ref-name>
        <jndi-name>jnp://localhost:1099/RSASignSession</jndi-name>
  </ejb-ref> 
  <ejb-ref>
     <ejb-ref-name>CertificateStoreSession</ejb-ref-name>
     <jndi-name>jnp://localhost:1099/CertificateStoreSession</jndi-name>
  </ejb-ref> 
  <ejb-ref>
     <ejb-ref-name>UserAdminSession</ejb-ref-name>
     <jndi-name>jnp://localhost:1099/UserAdminSession</jndi-name>
  </ejb-ref> 
  <ejb-ref>
     <ejb-ref-name>RaAdminSession</ejb-ref-name>
     <jndi-name>jnp://localhost:1099/RaAdminSession</jndi-name>
  </ejb-ref>   
</jboss-web>
