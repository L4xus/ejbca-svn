<?xml version="1.0"?>

<!DOCTYPE weblogic-rdbms-jar PUBLIC
 '-//BEA Systems, Inc.//DTD WebLogic 7.0.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls700/dtd/weblogic-rdbms20-persistence-700.dtd'>

<weblogic-rdbms-jar>

    <weblogic-rdbms-bean>
        <ejb-name>AdminGroupData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>ADMINGROUP</table-name>
            <field-map>
                <cmp-field>adminGroupName</cmp-field>
                <dbms-column>adminGroupName</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-bean>
        <ejb-name>AdminEntityData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>ADMINENTITY</table-name>
            <field-map>
                <cmp-field>pK</cmp-field>
                <dbms-column>pK</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>matchWith</cmp-field>
                <dbms-column>matchWith</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>matchType</cmp-field>
                <dbms-column>matchType</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>matchValue</cmp-field>
                <dbms-column>matchValue</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-bean>
        <ejb-name>AccessRulesData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>ACCESSRULES</table-name>
            <field-map>
                <cmp-field>pK</cmp-field>
                <dbms-column>pK</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>resource</cmp-field>
                <dbms-column>resource</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>accessRule</cmp-field>
                <dbms-column>accessRule</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-bean>
        <ejb-name>AdminPreferencesData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>ADMINPREFS</table-name>
            <field-map>
                <cmp-field>id</cmp-field>
                <dbms-column>id</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>data</cmp-field>
                <dbms-column>data</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-bean>
        <ejb-name>AvailableAccessRulesData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>AVAILABLEACCESSRULES</table-name>
            <field-map>
                <cmp-field>name</cmp-field>
                <dbms-column>name</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-bean>
        <ejb-name>EndEntityProfileData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>ENDENTITYPROFILE</table-name>
            <field-map>
                <cmp-field>id</cmp-field>
                <dbms-column>id</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>profileName</cmp-field>
                <dbms-column>profileName</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>data</cmp-field>
                <dbms-column>data</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-bean>
        <ejb-name>GlobalConfigurationData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>GLOBALCONFIG</table-name>
            <field-map>
                <cmp-field>data</cmp-field>
                <dbms-column>data</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>configurationId</cmp-field>
                <dbms-column>configurationId</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-bean>
        <ejb-name>UserData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>USERDATA</table-name>
            <field-map>
                <cmp-field>username</cmp-field>
                <dbms-column>username</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>clearPassword</cmp-field>
                <dbms-column>clearPassword</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>passwordHash</cmp-field>
                <dbms-column>passwordHash</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>subjectDN</cmp-field>
                <dbms-column>subjectDN</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>subjectAltName</cmp-field>
                <dbms-column>subjectAltName</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>subjectEmail</cmp-field>
                <dbms-column>subjectEmail</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>status</cmp-field>
                <dbms-column>status</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>type</cmp-field>
                <dbms-column>type</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>timeCreated</cmp-field>
                <dbms-column>timeCreated</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>timeModified</cmp-field>
                <dbms-column>timeModified</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>endEntityProfileId</cmp-field>
                <dbms-column>endEntityProfileId</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>certificateProfileId</cmp-field>
                <dbms-column>certificateProfileId</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>tokenType</cmp-field>
                <dbms-column>tokenType</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>hardTokenIssuerId</cmp-field>
                <dbms-column>hardTokenIssuerId</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>keyStorePassword</cmp-field>
                <dbms-column>keyStorePassword</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-relation>
      <relation-name>AdminGroupDataToAccessRules</relation-name>
        <weblogic-relationship-role>
          <relationship-role-name>AccessRulesData</relationship-role-name>
          <relationship-role-map>
                <column-map>
                  <foreign-key-column>adminGroupName</foreign-key-column>
                  <key-column>adminGroupName</key-column>
                </column-map>
          </relationship-role-map>
        </weblogic-relationship-role>
    </weblogic-rdbms-relation>

    <weblogic-rdbms-relation>
      <relation-name>AdminGroupDataToAdminEntities</relation-name>
        <weblogic-relationship-role>
          <relationship-role-name>AdminEntityData</relationship-role-name>
          <relationship-role-map>
                <column-map>
                  <foreign-key-column>adminGroupName</foreign-key-column>
                  <key-column>adminGroupName</key-column>
                </column-map>
          </relationship-role-map>
        </weblogic-relationship-role>
    </weblogic-rdbms-relation>

    <create-default-dbms-tables>False</create-default-dbms-tables>
</weblogic-rdbms-jar>
