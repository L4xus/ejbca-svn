<?xml version="1.0"?>

<jbosscmp-jdbc>

    <defaults>
        <datasource>java:/DefaultDS</datasource>
        <datasource-mapping>Sybase</datasource-mapping>
    </defaults>

    <!-- "rule" is a reserved word in Sybase-->
    <enterprise-beans>
        <entity>
            <ejb-name>AccessRulesData</ejb-name>
            <cmp-field>
            <field-name>rule</field-name>
            <column-name>rule_</column-name>
            </cmp-field>
        </entity>
    </enterprise-beans>

</jbosscmp-jdbc>

