<?xml version="1.0"?>

<!DOCTYPE weblogic-rdbms-jar PUBLIC
 '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd'>

<weblogic-rdbms-jar>

    <weblogic-rdbms-bean>
        <ejb-name>AdminGroupData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>ADMINGROUPDATA</table-name>
            <field-map>
                <cmp-field>pK</cmp-field>
                <dbms-column>pK</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>adminGroupName</cmp-field>
                <dbms-column>adminGroupName</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>cAId</cmp-field>
                <dbms-column>cAId</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>
    
    <weblogic-rdbms-bean>
        <ejb-name>AdminEntityData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>ADMINENTITYDATA</table-name>
            <field-map>
                <cmp-field>pK</cmp-field>
                <dbms-column>pK</dbms-column>
            </field-map>

            <field-map>
                <cmp-field>adminGroupFK</cmp-field>
                <dbms-column>adminGroupFK</dbms-column>
            </field-map>

            <field-map>
                <cmp-field>matchWith</cmp-field>
                <dbms-column>matchWith</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>matchType</cmp-field>
                <dbms-column>matchType</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>matchValue</cmp-field>
                <dbms-column>matchValue</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>    
    
    <weblogic-rdbms-bean>
        <ejb-name>AccessRulesData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>ACCESSRULESDATA</table-name>
            <field-map>
                <cmp-field>pK</cmp-field>
                <dbms-column>pK</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>adminGroupFK</cmp-field>
                <dbms-column>adminGroupFK</dbms-column>
            </field-map>
<!--
            <field-map>
                <cmp-field>resource</cmp-field>
                <dbms-column>resource</dbms-column>
            </field-map>
-->

            <field-map>
                <cmp-field>accessRule</cmp-field>
                <dbms-column>accessRule</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>isRecursive</cmp-field>
                <dbms-column>isRecursive</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>rule</cmp-field>
                <dbms-column>rule</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>   
    
    
    <weblogic-rdbms-bean>
        <ejb-name>AuthorizationTreeUpdateData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>AUTHORIZATIONTREEUPDATEDATA</table-name>
            <field-map>
                <cmp-field>pK</cmp-field>
                <dbms-column>pK</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>authorizationTreeUpdateNumber</cmp-field>
                <dbms-column>authorizationTreeUpdateNumber</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>   
    
    

    <weblogic-rdbms-relation>
      <relation-name>AdminGroupDataToAccessRules</relation-name>
        <weblogic-relationship-role>
          <relationship-role-name>AccessRulesData</relationship-role-name>
          <relationship-role-map>
                <column-map>
                  <foreign-key-column>AdminGroupFK</foreign-key-column><!-- pfa -->
                  <key-column>pK</key-column>
                </column-map>
          </relationship-role-map>
        </weblogic-relationship-role>
    </weblogic-rdbms-relation>

    <weblogic-rdbms-relation>
      <relation-name>AdminGroupDataToAdminEntities</relation-name>
        <weblogic-relationship-role>
          <relationship-role-name>AdminEntityData</relationship-role-name>
          <relationship-role-map>
                <column-map>
                  <foreign-key-column>AdminGroupFK</foreign-key-column><!-- pfa -->
                  <key-column>pK</key-column>
                </column-map>
          </relationship-role-map>
        </weblogic-relationship-role>
    </weblogic-rdbms-relation>

<!--    <create-default-dbms-tables>AlterOrCreate</create-default-dbms-tables> -->
</weblogic-rdbms-jar>
