<?xml version="1.0"?>

<!DOCTYPE weblogic-rdbms-jar PUBLIC
 '-//BEA Systems, Inc.//DTD WebLogic 7.0.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls700/dtd/weblogic-rdbms20-persistence-700.dtd'>

<weblogic-rdbms-jar>

    <weblogic-rdbms-bean>
        <ejb-name>LogEntryData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>LOGENTRYDATA</table-name>
            <field-map>
                <cmp-field>id</cmp-field>
                <dbms-column>id</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>adminType</cmp-field>
                <dbms-column>adminType</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>adminData</cmp-field>
                <dbms-column>adminData</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>module</cmp-field>
                <dbms-column>module</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>time</cmp-field>
                <dbms-column>time</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>certificateSNR</cmp-field>
                <dbms-column>certificateSNR</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>username</cmp-field>
                <dbms-column>username</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>event</cmp-field>
                <dbms-column>event</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>comment</cmp-field>
                <dbms-column>comment</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-bean>
        <ejb-name>LogConfigurationData</ejb-name>
        <data-source-name>DefaultDS</data-source-name>
        <table-map>
            <table-name>LOGCONFIGURATIONDATA</table-name>
            <field-map>
                <cmp-field>id</cmp-field>
                <dbms-column>id</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>logConfiguration</cmp-field>
                <dbms-column>logConfiguration</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>logEntryRowNumber</cmp-field>
                <dbms-column>logEntryRowNumber</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>

    <create-default-dbms-tables>False</create-default-dbms-tables>
</weblogic-rdbms-jar>
