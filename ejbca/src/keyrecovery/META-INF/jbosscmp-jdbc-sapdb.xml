<?xml version="1.0"?>

<jbosscmp-jdbc>

    <defaults>
        <datasource>java:/DefaultDS</datasource>
        <datasource-mapping>SapDB</datasource-mapping>
    </defaults>

<enterprise-beans>

    <entity>
        <ejb-name>KeyRecoveryData</ejb-name>
        <cmp-field>
            <field-name>keyData</field-name>
            <jdbc-type>LONGVARCHAR</jdbc-type>
            <sql-type>VARCHAR(2000)</sql-type>
        </cmp-field>
    </entity>

</enterprise-beans>

</jbosscmp-jdbc>
