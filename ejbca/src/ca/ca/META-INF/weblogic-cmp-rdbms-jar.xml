<?xml version="1.0"?>

<!DOCTYPE weblogic-rdbms-jar PUBLIC
 '-//BEA Systems, Inc.//DTD WebLogic 7.0.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls700/dtd/weblogic-rdbms20-persistence-700.dtd'>

<weblogic-rdbms-jar>

        <weblogic-rdbms-bean>
          <ejb-name>CertificateData</ejb-name>
          <data-source-name>DefaultDS</data-source-name>
          <table-map>
            <table-name>CERTIFICATEDATA</table-name>
            <field-map>
               <cmp-field>base64Cert</cmp-field>
               <dbms-column>base64Cert</dbms-column>
            </field-map>
            <field-map>
               <cmp-field>fingerprint</cmp-field>
               <dbms-column>fingerprint</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>subjectDN</cmp-field>
              <dbms-column>subjectDN</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>issuerDN</cmp-field>
              <dbms-column>issuerDN</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>cAFingerprint</cmp-field>
              <dbms-column>cAFingerprint</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>serialNumber</cmp-field>
              <dbms-column>serialNumber</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>status</cmp-field>
              <dbms-column>status</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>type</cmp-field>
              <dbms-column>type</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>expireDate</cmp-field>
              <dbms-column>expireDate</dbms-column>
            </field-map>
              <field-map>
                <cmp-field>username</cmp-field>
                <dbms-column>username</dbms-column>
              </field-map>
            <field-map>
              <cmp-field>revocationDate</cmp-field>
              <dbms-column>revocationDate</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>revocationReason</cmp-field>
              <dbms-column>revocationReason</dbms-column>
            </field-map>
          </table-map>
        </weblogic-rdbms-bean>

        <weblogic-rdbms-bean>
              <ejb-name>CertificateProfileData</ejb-name>
              <data-source-name>DefaultDS</data-source-name>
              <table-map>
                <table-name>CERTIFICATEPROFILEDATA</table-name>
                <field-map>
                   <cmp-field>id</cmp-field>
                   <dbms-column>id</dbms-column>
                </field-map>
                <field-map>
                   <cmp-field>certificateProfileName</cmp-field>
                   <dbms-column>certificateProfileName</dbms-column>
                </field-map>
                <field-map>
                   <cmp-field>data</cmp-field>
                   <dbms-column>data</dbms-column>
                </field-map>
              </table-map>
        </weblogic-rdbms-bean>

        <weblogic-rdbms-bean>
          <ejb-name>CRLData</ejb-name>
            <data-source-name>DefaultDS</data-source-name>
          <table-map>
            <table-name>CRLDATA</table-name>
            <field-map>
               <cmp-field>base64Crl</cmp-field>
               <dbms-column>base64Crl</dbms-column>
            </field-map>
            <field-map>
               <cmp-field>fingerprint</cmp-field>
               <dbms-column>fingerprint</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>cRLNumber</cmp-field>
              <dbms-column>cRLNumber</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>issuerDN</cmp-field>
              <dbms-column>issuerDN</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>cAFingerprint</cmp-field>
              <dbms-column>cAFingerprint</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>thisUpdate</cmp-field>
              <dbms-column>thisUpdate</dbms-column>
            </field-map>
            <field-map>
              <cmp-field>nextUpdate</cmp-field>
              <dbms-column>nextUpdate</dbms-column>
            </field-map>
          </table-map>
        </weblogic-rdbms-bean>

    <create-default-dbms-tables>False</create-default-dbms-tables>
</weblogic-rdbms-jar>
